{
  "name": "valorant-tips-api",
  "version": "4.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "API_VERSION=$(node -p -e \"require('./package.json').version\") node dist/server.js",
    "dev": " yarn && API_VERSION=$(node -p -e \"require('./package.json').version\") ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/infrastructure/api/server.ts",
    "format": "yarn prettier --write .",
    "test:outside": "make build-db-test && jest --runInBand --verbose",
    "test": "jest --runInBand --verbose",
    "test:watch": "jest --watchAll --runInBand --verbose ",
    "test:workflow:coverage": "jest --coverage --runInBand",
    "lint": "eslint -c ./.eslintrc.json && tsc --noEmit",
    "prepare": "husky install",
    "upgrade-packages": "yarn upgrade-interactive --latest",
    "depcheck-packages": "npx depcheck"
  },
  "engines": {
    "yarn": ">=1",
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielogregorio/valorant-tips-api.git"
  },
  "author": "Gabriel Greg√≥rio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabrielogregorio/valorant-tips-api/issues"
  },
  "homepage": "https://github.com/gabrielogregorio/valorant-tips-api#readme",
  "dependencies": {
    "cloudinary": "^2.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "sanitize-html": "^2.11.0",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/mongo-sanitize": "^1.0.3",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "@types/sanitize-html": "^2.11.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "13.1.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": []
  }
}
